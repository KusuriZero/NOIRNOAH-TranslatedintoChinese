dent":0,"parameters":["particle set city3 weather def -2"]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["你凭什么这么说我？你有什么证据吗？"]},{"code":401,"indent":0,"parameters":["谁知道是谁毁灭了人类？\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["雪儿"]},{"code":401,"indent":0,"parameters":["……因为正好遇见您,我想问问。"]},{"code":401,"indent":0,"parameters":["是什么原因让你想要毁灭人类？\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["……故意把我从沉睡中唤醒的不是别人,正是人类。\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["而我,曾经还被人类利用过。"]},{"code":401,"indent":0,"parameters":["如果你不相信我,那没关系。\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["雪儿"]},{"code":401,"indent":0,"parameters":["不,我相信。\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["……\""]},{"code":230,"indent":0,"parameters":[90]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["……‘绿眼之神’当初被人类封印,"]},{"code":401,"indent":0,"parameters":["那是一个在沉睡时吸干精力的封印。\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["人类剥夺了我所有的自由。"]},{"code":401,"indent":0,"parameters":["我忍不住讨厌人类。\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"parameters":["嗯……然而在我醒来之前,地球已经处于崩溃的边缘。\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["雪儿"]},{"code":401,"indent":0,"parameters":["崩溃？\""]},{"code":101,"indent":0,"parameters":["",0,0,2]},{"code":401,"indent":0,"parameters":["????"]},{"code":401,"indent":0,"pa